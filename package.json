{
  "type": "module",
  "name": "trend2zero",
  "version": "1.0.1",
  "description": "Comprehensive Vercel Build and Testing Configuration with Advanced Caching",
  "bin": {
    "trend2zero": "./scripts/cli.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:performance": "node scripts/analyze-build-performance.js",
    "build:cache:clear": "rm -rf .next/cache",
    "build:cache:analyze": "node scripts/analyze-build-performance.js --cache-only",
    "build:cache:validate": "node scripts/validate-build-cache.js",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "nyc --reporter=html --reporter=text jest",
    "test:e2e": "playwright test",
    "test:visual": "playwright test tests/home-page-visual.spec.ts",
    "test:visual:update": "playwright test tests/home-page-visual.spec.ts --update-snapshots",
    "test:visual:report": "playwright show-report",
    "test:performance": "lighthouse http://localhost:3000 --view",
    "test:crypto-symbols": "node scripts/test-coingecko-symbols.js",
    "test:crypto-historical": "node scripts/test-coingecko-historical.js",
    "test:crypto": "npm run test:crypto-symbols && npm run test:crypto-historical",
    "cache:metrics": "node scripts/cache-handler.js --metrics",
    "cache:prune": "node scripts/cache-handler.js --prune",
    "vercel:deploy": "vercel",
    "vercel:deploy:prod": "vercel --prod",
    "monitor:build": "./scripts/run-continuous-monitor.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@types/dotenv": "^6.1.1",
    "@types/highcharts": "^5.0.44",
    "@types/react-dom": "^19.1.2",
    "@vercel/speed-insights": "^1.0.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "highcharts": "^12.2.0",
    "highcharts-react-official": "^3.2.1",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^5.0.5",
    "mongoose": "^8.13.2",
    "next": "^14.2.28",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.4.1",
    "winston": "^3.17.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/runtime": "^7.27.0",
    "@eslint/js": "^9.0.0",
    "@next/bundle-analyzer": "^14.2.28",
    "@playwright/test": "1.51.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "callsites": "^4.2.0",
    "cssnano": "^7.0.6",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.2.28",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.5.1",
    "mongodb-memory-server": "^10.1.4",
    "nyc": "^15.1.0",
    "playwright-core": "1.51.1",
    "postcss": "8.4.31",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "~5.5.0",
    "vite": "^6.2.6",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "cacheManagement": {
    "maxSizeMB": 500,
    "expirationDays": 7
  }
}
