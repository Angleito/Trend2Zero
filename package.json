{
  "name": "trend2zero",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm install --legacy-peer-deps && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm install --legacy-peer-deps && jest __tests__",
    "test:watch": "jest __tests__ --watch",
    "test:coverage": "jest __tests__ --coverage",
    "test:ci": "jest __tests__ --ci --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --config=playwright.config.js --debug",
    "test:e2e:verbose": "playwright test --config=playwright.config.js -j 1 --reporter=list",
    "test:e2e:report": "playwright show-report test-results/html-report",
    "test:all": "npm run test:ci && npm run test:e2e",
    "prepare": "husky install",
    "run-all-tests": "bash scripts/run-all-tests.sh",
    "test:comprehensive": "bash scripts/run-tests.sh",
    "test:mcp:logs": "node scripts/analyze-mcp-logs.js",
    "test:server:start": "node scripts/start-dev-server.js",
    "migrate:strapi": "node scripts/migrate-to-strapi.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "13.5.11",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "log4js": "^6.9.1",
    "postcss": "^8.4.35",
    "react-test-renderer": "^19.1.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@agentdeskai/browser-tools-mcp": "^1.2.0",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@smithery/sdk": "^1.0.4",
    "@strapi/blocks-react-renderer": "^1.0.0",
    "axios": "^1.8.4",
    "child_process": "^1.0.2",
    "dotenv": "^16.4.7",
    "highcharts": "^11.4.0",
    "highcharts-react-official": "^3.2.1",
    "lightweight-charts": "^4.1.2",
    "lru-cache": "^10.2.0",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.2",
    "next": "13.5.11",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "url": "^0.11.4",
    "zustand": "^5.0.3"
  },
  "overrides": {
    "@types/react": "^18.3.0"
  },
  "resolutions": {
    "@types/react": "^18.3.0"
  },
  "playwright": {
    "globalSetup": "tests/global-setup.js",
    "globalTeardown": "tests/global-teardown.js"
  }
}
